{
  "name": "sheepsimjavascript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "devDependencies": {
    "@types/node": "^20.6.0",
    "canvas": "^2.11.2",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run buildTSC && npm run buildCSS && npm run buildHTML",
    "buildWindows": "npm run buildTSCWindows && npm run buildCSSWindows && npm run buildHTMLWindows",
    "buildTSC" : "tsc -p tsconfigProd.json",
    "buildCSS": "cp -r css/ buildProd/css/",
    "buildHTML": "cp index.html buildProd/",
    "buildTSCWindows" : "tsc",
    "buildCSSWindows": "xcopy /i /y css\\* build\\css\\",
    "buildHTMLWindows": "xcopy /i /y index.html build\\"
  },
